ansition: opacity 0.3s ease;\r\n  &:hover {\r\n    opacity: 0.8;\r\n  }\r\n  i {\r\n    font-size: 1.2rem;\r\n    color: #25D366;\r\n  }\r\n}\r\n.app-footer .footer-content {\r\n    flex-direction: column;\r\n    text-align: center;\r\n  }\r\n}\r\n</style>\r\n<style>\r\n:root {\r\n  --primary-color: #1e3c72;\r\n  --secondary-color: #2a5298;\r\n}\r\n\r\n/* Aplica as cores globais */\r\n.nav-button, .login-button, .submit-button {\r\n  background-color: var(--primary-color) !important;\r\n}\r\n\r\n.nav-button:hover, .login-button:hover, .submit-button:hover {\r\n  background-color: var(--secondary-color) !important;\r\n}\r\n\r\n/* Outros estilos globais que usam as cores */\r\n.app-header {\r\n  background: linear-gradient(135deg, var(--primary-color) 0%, var(--secondary-color) 100%);\r\n}\r\n\r\n.login-container {\r\n  background: linear-gradient(135deg, var(--primary-color) 0%, var(--secondary-color) 100%);\r\n}\r\n</style>\r\n"],"mappings":";AAkFA,SAASA,GAAG,EAAEC,SAAS,EAAEC,KAAK,EAAEC,QAAO,QAAS,KAAI;AACpD,SAASC,SAAQ,QAAS,YAAW;AAErC,eAAe;EACbC,KAAK,WAALA,KAAKA,CAAA,EAAG;IACN,IAAMC,MAAK,GAAIF,SAAS,CAAC;IACzB,IAAMG,eAAc,GAAIP,GAAG,CAAC,KAAK;IACjC,IAAMQ,UAAS,GAAIR,GAAG,CAAC,EAAE;IACzB,IAAMS,UAAS,GAAIT,GAAG,CAAC,EAAE;IACzB,IAAMU,WAAU,GAAIP,QAAQ,CAAC;MAAA,OAAMG,MAAM,CAACK,YAAY,CAACC,KAAK,CAACC,IAAG,KAAM,QAAQ;IAAA;IAC9E,IAAMC,OAAM,GAAId,GAAG,CAAC,KAAK;IAEzB,IAAMe,cAAa,GAAI,SAAjBA,cAAaA,CAAA,EAAU;MAC3B,IAAI;QACF;QACAP,UAAU,CAACI,KAAI,GAAII,YAAY,CAACC,OAAO,CAAC,YAAY,KAAK,EAAC;QAC1DR,UAAU,CAACG,KAAI,GAAII,YAAY,CAACC,OAAO,CAAC,YAAY,KAAK,EAAC;QAC1DH,OAAO,CAACF,KAAI,GAAII,YAAY,CAACC,OAAO,CAAC,SAAS,MAAM,MAAK;MAC3D,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,mCAAmC,EAAEA,KAAK;MAC1D;IACF;IAEAjB,SAAS,CAAC,YAAM;MACd;MACA,IAAMmB,UAAS,GAAIJ,YAAY,CAACC,OAAO,CAAC,iBAAiB,MAAM,MAAK;MACpEV,eAAe,CAACK,KAAI,GAAIQ,UAAS;MACjC,IAAIA,UAAU,EAAE;QACdL,cAAc,CAAC;MACjB,OAAO;QACLT,MAAM,CAACe,IAAI,CAAC,QAAQ;MACtB;IACF,CAAC;IAEDC,MAAM,CAACC,gBAAgB,CAAC,SAAS,EAAE,UAACC,CAAC,EAAK;MACxC,IAAIA,CAAC,CA