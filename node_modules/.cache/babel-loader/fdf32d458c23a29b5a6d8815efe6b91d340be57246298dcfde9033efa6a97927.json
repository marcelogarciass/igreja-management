{"ast":null,"code":"import { ref } from 'vue';\nexport default {\n  name: 'MembersView',\n  setup() {\n    const memberForm = ref({\n      name: '',\n      email: '',\n      phone: ''\n    });\n    const members = ref([]);\n    const isEditing = ref(false);\n    const editingId = ref(null);\n    const handleSubmit = () => {\n      if (isEditing.value) {\n        // Update existing member\n        const index = members.value.findIndex(m => m.id === editingId.value);\n        if (index !== -1) {\n          members.value[index] = {\n            ...members.value[index],\n            ...memberForm.value\n          };\n        }\n      } else {\n        // Add new member\n        members.value.push({\n          id: Date.now(),\n          ...memberForm.value\n        });\n      }\n      resetForm();\n    };\n    const editMember = member => {\n      memberForm.value = {\n        ...member\n      };\n      isEditing.value = true;\n      editingId.value = member.id;\n    };\n    const deleteMember = id => {\n      if (confirm('Deseja realmente excluir este membro?')) {\n        members.value = members.value.filter(m => m.id !== id);\n      }\n    };\n    const resetForm = () => {\n      memberForm.value = {\n        name: '',\n        email: '',\n        phone: ''\n      };\n      isEditing.value = false;\n      editingId.value = null;\n    };\n    const cancelEdit = () => {\n      resetForm();\n    };\n    return {\n      memberForm,\n      members,\n      isEditing,\n      handleSubmit,\n      editMember,\n      deleteMember,\n      cancelEdit\n    };\n  }\n};","map":{"version":3,"names":["ref","name","setup","memberForm","email","phone","members","isEditing","editingId","handleSubmit","value","index","findIndex","m","id","push","Date","now","resetForm","editMember","member","deleteMember","confirm","filter","cancelEdit"],"sources":["C:\\Users\\marce\\Documents\\Projeto Garcia\\src\\views\\MembersView.vue"],"sourcesContent":["<template>\r\n  <div class=\"members-view\">\r\n    <h1>Cadastro de Membros</h1>\r\n    \r\n    <!-- Member Form -->\r\n    <div class=\"member-form\">\r\n      <h2>{{ isEditing ? 'Editar Membro' : 'Novo Membro' }}</h2>\r\n      <form @submit.prevent=\"handleSubmit\">\r\n        <div class=\"form-group\">\r\n          <label for=\"name\">Nome*</label>\r\n          <input \r\n            id=\"name\"\r\n            v-model=\"memberForm.name\"\r\n            type=\"text\"\r\n            required\r\n          />\r\n        </div>\r\n        \r\n        <div class=\"form-group\">\r\n          <label for=\"email\">E-mail*</label>\r\n          <input \r\n            id=\"email\"\r\n            v-model=\"memberForm.email\"\r\n            type=\"email\"\r\n            required\r\n          />\r\n        </div>\r\n        \r\n        <div class=\"form-group\">\r\n          <label for=\"phone\">Telefone*</label>\r\n          <input \r\n            id=\"phone\"\r\n            v-model=\"memberForm.phone\"\r\n            type=\"tel\"\r\n            required\r\n          />\r\n        </div>\r\n        \r\n        <div class=\"form-actions\">\r\n          <button type=\"submit\">\r\n            {{ isEditing ? 'Atualizar' : 'Cadastrar' }}\r\n          </button>\r\n          <button \r\n            v-if=\"isEditing\"\r\n            type=\"button\"\r\n            @click=\"cancelEdit\"\r\n          >\r\n            Cancelar\r\n          </button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n\r\n    <!-- Members List -->\r\n    <div class=\"members-list\">\r\n      <h2>Membros Cadastrados</h2>\r\n      <table>\r\n        <thead>\r\n          <tr>\r\n            <th>Nome</th>\r\n            <th>E-mail</th>\r\n            <th>Telefone</th>\r\n            <th>Ações</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"member in members\" :key=\"member.id\">\r\n            <td>{{ member.name }}</td>\r\n            <td>{{ member.email }}</td>\r\n            <td>{{ member.phone }}</td>\r\n            <td>\r\n              <button @click=\"editMember(member)\">Editar</button>\r\n              <button @click=\"deleteMember(member.id)\">Excluir</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue'\r\n\r\nexport default {\r\n  name: 'MembersView',\r\n  setup() {\r\n    const memberForm = ref({\r\n      name: '',\r\n      email: '',\r\n      phone: ''\r\n    })\r\n    \r\n    const members = ref([])\r\n    const isEditing = ref(false)\r\n    const editingId = ref(null)\r\n\r\n    const handleSubmit = () => {\r\n      if (isEditing.value) {\r\n        // Update existing member\r\n        const index = members.value.findIndex(m => m.id === editingId.value)\r\n        if (index !== -1) {\r\n          members.value[index] = {\r\n            ...members.value[index],\r\n            ...memberForm.value\r\n          }\r\n        }\r\n      } else {\r\n        // Add new member\r\n        members.value.push({\r\n          id: Date.now(),\r\n          ...memberForm.value\r\n        })\r\n      }\r\n      \r\n      resetForm()\r\n    }\r\n\r\n    const editMember = (member) => {\r\n      memberForm.value = { ...member }\r\n      isEditing.value = true\r\n      editingId.value = member.id\r\n    }\r\n\r\n    const deleteMember = (id) => {\r\n      if (confirm('Deseja realmente excluir este membro?')) {\r\n        members.value = members.value.filter(m => m.id !== id)\r\n      }\r\n    }\r\n\r\n    const resetForm = () => {\r\n      memberForm.value = {\r\n        name: '',\r\n        email: '',\r\n        phone: ''\r\n      }\r\n      isEditing.value = false\r\n      editingId.value = null\r\n    }\r\n\r\n    const cancelEdit = () => {\r\n      resetForm()\r\n    }\r\n\r\n    return {\r\n      memberForm,\r\n      members,\r\n      isEditing,\r\n      handleSubmit,\r\n      editMember,\r\n      deleteMember,\r\n      cancelEdit\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.members-view {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.member-form {\r\n  background-color: #f5f5f5;\r\n  padding: 1.5rem;\r\n  border-radius: 8px;\r\n  margin-bottom: 2rem;\r\n\r\n  form {\r\n    display: grid;\r\n    gap: 1rem;\r\n  }\r\n}\r\n\r\n.form-group {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 0.5rem;\r\n\r\n  label {\r\n    font-weight: bold;\r\n  }\r\n\r\n  input {\r\n    padding: 0.5rem;\r\n    border: 1px solid #ddd;\r\n    border-radius: 4px;\r\n  }\r\n}\r\n\r\n.form-actions {\r\n  display: flex;\r\n  gap: 1rem;\r\n  margin-top: 1rem;\r\n\r\n  button {\r\n    padding: 0.5rem 1rem;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    \r\n    &:first-child {\r\n      background-color: #4CAF50;\r\n      color: white;\r\n    }\r\n    \r\n    &:last-child {\r\n      background-color: #f44336;\r\n      color: white;\r\n    }\r\n  }\r\n}\r\n\r\n.members-list {\r\n  table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n    \r\n    th, td {\r\n      padding: 0.75rem;\r\n      text-align: left;\r\n      border-bottom: 1px solid #ddd;\r\n    }\r\n    \r\n    th {\r\n      background-color: #f5f5f5;\r\n    }\r\n    \r\n    button {\r\n      margin-right: 0.5rem;\r\n      padding: 0.25rem 0.5rem;\r\n      border: none;\r\n      border-radius: 4px;\r\n      cursor: pointer;\r\n      \r\n      &:first-child {\r\n        background-color: #2196F3;\r\n        color: white;\r\n      }\r\n      \r\n      &:last-child {\r\n        background-color: #f44336;\r\n        color: white;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"mappings":"AAkFA,SAASA,GAAE,QAAS,KAAI;AAExB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAKA,CAAA,EAAG;IACN,MAAMC,UAAS,GAAIH,GAAG,CAAC;MACrBC,IAAI,EAAE,EAAE;MACRG,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE;IACT,CAAC;IAED,MAAMC,OAAM,GAAIN,GAAG,CAAC,EAAE;IACtB,MAAMO,SAAQ,GAAIP,GAAG,CAAC,KAAK;IAC3B,MAAMQ,SAAQ,GAAIR,GAAG,CAAC,IAAI;IAE1B,MAAMS,YAAW,GAAIA,CAAA,